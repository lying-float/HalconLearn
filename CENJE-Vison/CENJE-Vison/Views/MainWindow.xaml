<Window
    x:Class="CENJE_Vison.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:convert="clr-namespace:CENJE_Vison.Views.Convert"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:h="clr-namespace:HalconDotNet;assembly=halcondotnet"
    xmlns:local="clr-namespace:CENJE_Vison.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="CCD钉卷机视觉检测系统"
    Width="1366"
    Height="768"
    Background="#6D819A"
    DataContext="{Binding Source={StaticResource Locator}, Path=Main}"
    FontFamily="SimSun"
    ResizeMode="CanResizeWithGrip"
    WindowStartupLocation="CenterScreen"
    WindowState="Maximized"
    mc:Ignorable="d">
    <b:Interaction.Triggers>
        <b:EventTrigger EventName="Loaded">
            <b:InvokeCommandAction Command="{Binding Load_Command}" />
        </b:EventTrigger>
        <b:EventTrigger EventName="Closed">
            <b:InvokeCommandAction Command="{Binding Closed_Command}" />
        </b:EventTrigger>
    </b:Interaction.Triggers>
    <FrameworkElement.Resources>
        <ResourceDictionary>
            <Style x:Key="ItemName" TargetType="{x:Type TextBlock}">
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="FontFamily" Value="SimSun" />
                <Setter Property="Margin" Value="6,6,0,6" />
                <Setter Property="FontSize" Value="14" />
            </Style>
            <Style TargetType="TextBox">
                <Setter Property="BorderBrush" Value="green" />
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TextBoxBase}">
                            <Border
                                x:Name="border"
                                Margin="1"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="4">
                                <Grid>
                                    <ScrollViewer
                                        x:Name="PART_ContentHost"
                                        Margin="1"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Focusable="False"
                                        HorizontalScrollBarVisibility="Disabled"
                                        VerticalScrollBarVisibility="Disabled" />
                                </Grid>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </FrameworkElement.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <Grid.Resources>
            <convert:ItemBoolConvert x:Key="ItemBoolConvert" />
            <Style TargetType="MenuItem">
                <Setter Property="Foreground" Value="{Binding Path=Foreground, RelativeSource={RelativeSource AncestorType={x:Type Menu}}}" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="MenuItem">
                            <Border
                                x:Name="Border"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="1">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition x:Name="ColIcon" Width="auto" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition x:Name="ColNext" Width="auto" />
                                    </Grid.ColumnDefinitions>
                                    <Border BorderBrush="#22ffff" BorderThickness="0,0,1,0">
                                        <Grid
                                            x:Name="GlyphPanel"
                                            Grid.Column="0"
                                            VerticalAlignment="Center"
                                            Visibility="Hidden">
                                            <Path Data="F1 M 10.0,1.2 L 4.7,9.1 L 4.5,9.1 L 0,5.2 L 1.3,3.5 L 4.3,6.1L 8.3,0 L 10.0,1.2 Z" Fill="#00aaff" />
                                        </Grid>
                                    </Border>
                                    <ContentPresenter
                                        x:Name="HeaderHost"
                                        Grid.Column="1"
                                        Margin="8,0"
                                        VerticalAlignment="Center"
                                        ContentSource="Header" />
                                    <Grid
                                        x:Name="ArrowPanel"
                                        Grid.Column="2"
                                        Margin="5,0"
                                        VerticalAlignment="Center">
                                        <Path
                                            x:Name="ArrowPanelPath"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            Data="M0,0 L0,8 L4,4 z"
                                            Fill="{TemplateBinding Foreground}" />
                                    </Grid>
                                    <Popup
                                        x:Name="SubMenuPopup"
                                        Focusable="false"
                                        IsOpen="{Binding Path=IsSubmenuOpen, RelativeSource={RelativeSource TemplatedParent}}"
                                        Placement="Right">
                                        <Border
                                            x:Name="SubMenuBorder"
                                            Padding="2,2,2,2"
                                            BorderBrush="#FFFFFF"
                                            BorderThickness="1">
                                            <Grid x:Name="SubMenu">
                                                <StackPanel
                                                    Background="#ffffff"
                                                    IsItemsHost="True"
                                                    KeyboardNavigation.DirectionalNavigation="Cycle" />
                                            </Grid>
                                        </Border>
                                    </Popup>
                                </Grid>
                            </Border>

                            <ControlTemplate.Triggers>
                                <Trigger Property="Role" Value="TopLevelHeader">
                                    <Setter Property="Padding" Value="6,1" />
                                    <Setter Property="Background" Value="Transparent" />
                                    <Setter TargetName="SubMenuPopup" Property="Placement" Value="Bottom" />
                                    <Setter TargetName="ColIcon" Property="MinWidth" Value="0" />
                                    <Setter TargetName="ColNext" Property="Width" Value="Auto" />
                                    <Setter TargetName="GlyphPanel" Property="Visibility" Value="Collapsed" />
                                    <Setter TargetName="ArrowPanel" Property="Visibility" Value="Collapsed" />
                                </Trigger>
                                <Trigger Property="IsHighlighted" Value="True">
                                    <Setter Property="Background" Value="#ffaa00" />
                                </Trigger>
                                <MultiTrigger>
                                    <MultiTrigger.Conditions>
                                        <Condition Property="IsHighlighted" Value="True" />
                                        <Condition Property="Role" Value="TopLevelHeader" />
                                    </MultiTrigger.Conditions>
                                    <MultiTrigger.Setters>
                                        <Setter TargetName="Border" Property="Background" Value="Green" />
                                    </MultiTrigger.Setters>
                                </MultiTrigger>

                                <Trigger Property="Role" Value="TopLevelItem">
                                    <Setter Property="Padding" Value="6,1" />
                                    <Setter TargetName="ArrowPanel" Property="Visibility" Value="Collapsed" />
                                </Trigger>
                                <Trigger Property="Role" Value="SubmenuHeader">
                                    <Setter Property="Padding" Value="5,2" />
                                </Trigger>
                                <Trigger Property="Role" Value="SubmenuItem">
                                    <Setter Property="Padding" Value="5,2" />
                                    <Setter TargetName="ArrowPanel" Property="Visibility" Value="Collapsed" />
                                </Trigger>

                                <Trigger Property="IsChecked" Value="true">
                                    <Setter TargetName="GlyphPanel" Property="Visibility" Value="Visible" />
                                </Trigger>

                                <Trigger Property="IsEnabled" Value="false">
                                    <Setter Property="Foreground" Value="LightGray" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </Grid.Resources>

        <Menu
            Grid.Row="0"
            Grid.Column="0"
            Grid.ColumnSpan="3"
            Background="#FFF0F8FF"
            FontFamily="SimSun"
            FontSize="16">
            <MenuItem Name="monu" Header="执行">
                <MenuItem
                    Name="runAll"
                    Command="{Binding MenuItem_Command}"
                    Header="运行全部检测"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.isOpenAllChecked, Mode=TwoWay}" />
                <MenuItem
                    Name="stopAll"
                    Command="{Binding MenuItem_Command}"
                    Header="停止全部检测"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.isOpenAllChecked, Mode=TwoWay, Converter={StaticResource ItemBoolConvert}}" />
                <Separator />
                <MenuItem
                    Name="runLocation1"
                    Header="运行负极花瓣检测"
                    IsCheckable="{Binding MainViewSection.isOpenAllChecked}"
                    IsChecked="{Binding MainViewSection.IsOpenNegativeChecked, Mode=TwoWay}" />
                <MenuItem
                    Name="runLocation2"
                    Header="运行正极花瓣检测"
                    IsCheckable="{Binding MainViewSection.isOpenAllChecked}"
                    IsChecked="{Binding MainViewSection.IsOpenPositiveChecked, Mode=TwoWay}" />

                <MenuItem
                    Name="runLocation3"
                    Header="运行成品检测"
                    IsCheckable="{Binding MainViewSection.isOpenAllChecked}"
                    IsChecked="{Binding MainViewSection.IsOpenFinishChecked, Mode=TwoWay}" />
                <Separator />
                <Separator />
                <MenuItem
                    Name="projectSet"
                    Command="{Binding Formula_Command}"
                    Header="配方列表" />
                <Separator />
            </MenuItem>
            <MenuItem Name="debug" Header="调试">
                <MenuItem
                    Name="debugLocation1"
                    Command="{Binding SettingUp_Command}"
                    CommandParameter="{Binding ElementName=debugLocation1, Path=Name}"
                    Header="负极花瓣检测" />
                <MenuItem
                    Name="debugLocation2"
                    Command="{Binding SettingUp_Command}"
                    CommandParameter="{Binding ElementName=debugLocation2, Path=Name}"
                    Header="正极花瓣检测" />
                <MenuItem
                    Name="debugLocation3"
                    Command="{Binding SettingUp_Command}"
                    CommandParameter="{Binding ElementName=debugLocation3, Path=Name}"
                    Header="成品检测" />
            </MenuItem>
            <MenuItem Name="record" Header="记录">
                <MenuItem Name="ok" Header="OK">
                    <MenuItem
                        Name="okLocation1"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=okLocation1, Path=Name}"
                        Header="负极花瓣检测" />
                    <MenuItem
                        Name="okLocation2"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=okLocation2, Path=Name}"
                        Header="正极花瓣检测" />
                    <MenuItem
                        Name="okLocation3"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=okLocation3, Path=Name}"
                        Header="成品检测" />
                </MenuItem>
                <MenuItem Name="ng" Header="NG">
                    <MenuItem
                        Name="ngLocation1"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=ngLocation1, Path=Name}"
                        Header="负极花瓣检测" />
                    <MenuItem
                        Name="ngLocation2"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=ngLocation2, Path=Name}"
                        Header="正极花瓣检测" />
                    <MenuItem
                        Name="ngLocation3"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=ngLocation3, Path=Name}"
                        Header="成品检测" />
                </MenuItem>
                <MenuItem Name="all" Header="结果图像">
                    <MenuItem
                        Name="allLocation1"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=allLocation1, Path=Name}"
                        Header="负极花瓣检测" />
                    <MenuItem
                        Name="allLocation2"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=allLocation2, Path=Name}"
                        Header="正极花瓣检测" />
                    <MenuItem
                        Name="allLocation3"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=allLocation3, Path=Name}"
                        Header="成品检测" />
                </MenuItem>
                <MenuItem Name="faultImage" Header="Fault">
                    <MenuItem
                        Name="FaultLocation1"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=okLocation1, Path=Name}"
                        Header="负极花瓣检测" />
                    <MenuItem
                        Name="FaultLocation2"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=okLocation1, Path=Name}"
                        Header="正极花瓣检测" />
                    <MenuItem
                        Name="FaultLocation3"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=okLocation1, Path=Name}"
                        Header="成品检测" />
                </MenuItem>
                <Separator />
                <MenuItem
                    Name="fault"
                    Command="{Binding Record_Command}"
                    CommandParameter="{Binding ElementName=fault, Path=Name}"
                    Header="错误" />
                <Separator />
                <MenuItem
                    Name="log"
                    Command="{Binding Record_Command}"
                    CommandParameter="{Binding ElementName=log, Path=Name}"
                    Header="日志" />
                <MenuItem Name="data" Header="数据">
                    <MenuItem
                        Name="DataLocation1"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=DataLocation1, Path=Name}"
                        Header="负极花瓣检测" />
                    <MenuItem
                        Name="DataLocation2"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=DataLocation2, Path=Name}"
                        Header="正极花瓣检测" />
                    <MenuItem
                        Name="DataLocation3"
                        Command="{Binding Record_Command}"
                        CommandParameter="{Binding ElementName=DataLocation3, Path=Name}"
                        Header="成品检测" />
                </MenuItem>
            </MenuItem>
            <MenuItem Name="tool" Header="工具">
                <MenuItem
                    Name="realTimeMode"
                    Header="实时显示模式"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.RealTimeMode, Mode=TwoWay}" />
                <MenuItem Name="runCleaner" Header="运行清洁器" />
                <MenuItem
                    Name="TestData"
                    Header="数据测试开始"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.TestData, Mode=TwoWay}" />
                <MenuItem
                    Name="SaveNGData"
                    Header="存NG结果图片"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.SaveNGData, Mode=TwoWay}" />
                <MenuItem
                    Name="SaveOKData"
                    Header="存OK结果图片"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.SaveOKData, Mode=TwoWay}" />
                <MenuItem
                    Name="SaveOrData"
                    Header="存原图片"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.SaveOrData, Mode=TwoWay}" />
                <MenuItem
                    Name="isResultOut"
                    Header="屏蔽输出"
                    IsChecked="False">
                    <MenuItem
                        Name="negativeResultOut"
                        Header="负极屏蔽输出"
                        IsCheckable="False">
                        <MenuItem
                            Name="negativeFlower"
                            Header="屏蔽极花输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.NegativeFlower, Mode=TwoWay}" />
                        <MenuItem
                            Name="negativeAngle"
                            Header="屏蔽角度输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.NegativeAngle, Mode=TwoWay}" />
                        <MenuItem
                            Name="negativeL2"
                            Header="屏蔽L2输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.NegativeL2, Mode=TwoWay}" />
                        <MenuItem
                            Name="negativeSplit"
                            Header="屏蔽箔裂输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.NegativeSplit, Mode=TwoWay}" />
                        <MenuItem
                            Name="negativeNeedleHole"
                            Header="屏蔽铆偏输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.NegativeNeedleHole, Mode=TwoWay}" />
                        <MenuItem
                            Name="negativeAll"
                            Foreground="#FFFF0000"
                            Header="屏蔽所有输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.NegativeAll, Mode=TwoWay}" 
                            Command="{Binding MenuItemMask_Command}" CommandParameter="{Binding ElementName=negativeAll, Path=Name}"/>
                    </MenuItem>
                    <MenuItem
                        Name="positiveResultOut"
                        Header="正极屏蔽输出"
                        IsChecked="False">
                        <MenuItem
                            Name="positiveFlower"
                            Header="屏蔽极花输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveFlower, Mode=TwoWay}" />
                        <MenuItem
                            Name="positiveAngle"
                            Header="屏蔽角度输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveAngle, Mode=TwoWay}" />
                        <MenuItem
                            Name="positiveL2"
                            Header="屏蔽L2输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveL2, Mode=TwoWay}" />
                        <MenuItem
                            Name="positiveSplit"
                            Header="屏蔽箔裂输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveSplit, Mode=TwoWay}" />
                        <MenuItem
                            Name="positiveMoSplit"
                            Header="屏蔽底膜箔裂输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveMoSplit, Mode=TwoWay}" />
                        <MenuItem
                            Name="positiveNeedleHole"
                            Header="屏蔽铆偏输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveNeedleHole, Mode=TwoWay}" />
                        <MenuItem
                            Name="positiveAll"
                            Foreground="#FFFF0000"
                            Header="屏蔽所有输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PositiveAll, Mode=TwoWay}" 
                            Command="{Binding MenuItemMask_Command}" CommandParameter="{Binding ElementName=positiveAll, Path=Name}"/>
                    </MenuItem>
                    <MenuItem
                        Name="productionResultOut"
                        Header="成品屏蔽输出"
                        IsCheckable="False"
                        IsChecked="False">
                        <MenuItem
                            Name="productionHeight"
                            Header="屏蔽高度输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.ProductionHeight, Mode=TwoWay}" />
                        <MenuItem
                            Name="productionDiameter"
                            Header="屏蔽直径输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.ProductionDiameter, Mode=TwoWay}" />
                        <MenuItem
                            Name="pproductionfootDistanceExternal"
                            Header="屏蔽脚距输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.PproductionfootDistanceExternal, Mode=TwoWay}" />
                        <MenuItem
                            Name="productionfootOffset"
                            Header="屏蔽高低脚输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.ProductionfootOffset, Mode=TwoWay}" />
                        <MenuItem
                            Name="productionAll"
                            Foreground="#FFFF0000"
                            Header="屏蔽所有输出"
                            IsCheckable="True"
                            IsChecked="{Binding MainViewSection.ProductionAll, Mode=TwoWay}" 
                            Command="{Binding MenuItemMask_Command}" CommandParameter="{Binding ElementName=productionAll, Path=Name}"/>
                    </MenuItem>
                </MenuItem>
                <MenuItem Name="dataShow" Header="数据显示" />
                <MenuItem Name="lightAndIo" Header="光源与IO" />
                <MenuItem
                    Name="StartAtuo"
                    Header="开机自启动"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.StartAtuo, Mode=TwoWay}" />
                <MenuItem
                    Name="ClearTotal"
                    Header="统计清零"
                    IsCheckable="False" />
                <MenuItem
                    Name="ShowAlarm"
                    Header="启用错误提示"
                    IsCheckable="True"
                    IsChecked="{Binding MainViewSection.ShowAlarm, Mode=TwoWay}" />
            </MenuItem>
            <MenuItem Name="User" Header="用户">
                <MenuItem Name="CheckIn" Header="登入" />
                <MenuItem Name="CheckOut" Header="注销" />
            </MenuItem>
        </Menu>
        <Label
            Name="Userstate"
            Grid.Row="0"
            Grid.Column="2"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            Content="登入状态："
            FontSize="10" />
        <Viewbox
            Grid.Row="1"
            Grid.RowSpan="1"
            Grid.Column="0"
            Grid.ColumnSpan="3"
            Margin="-2,5,2,-5"
            Stretch="Fill">
            <Grid
                Background="#FFFFFFFF"
                Column="0"
                Row="1"
                RowSpan="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="79*" />
                    <ColumnDefinition Width="81*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="79*" />
                    <ColumnDefinition Width="79*" />
                    <ColumnDefinition Width="79*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                    <ColumnDefinition Width="80*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="30*" />
                    <RowDefinition Height="27*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                    <RowDefinition Height="31*" />
                </Grid.RowDefinitions>
                <h:HSmartWindowControlWPF
                    Name="location2HWindow"
                    Grid.Row="1"
                    Grid.RowSpan="11"
                    Grid.Column="0"
                    Grid.ColumnSpan="9"
                    BorderThickness="1"
                    HImagePart="{Binding}">
                    <h:HIconicDisplayObjectWPF IconicObject="{Binding PImage}" />
                </h:HSmartWindowControlWPF>

                <h:HSmartWindowControlWPF
                    Name="location1HWindow"
                    Grid.Row="1"
                    Grid.RowSpan="11"
                    Grid.Column="9"
                    Grid.ColumnSpan="9"
                    Height="337"
                    Margin="5,0,0,0"
                    VerticalAlignment="Top"
                    BorderThickness="1">
                    <h:HIconicDisplayObjectWPF IconicObject="{Binding NImage}" />
                </h:HSmartWindowControlWPF>
                <h:HSmartWindowControlWPF
                    Name="location3HWindow"
                    Grid.Row="1"
                    Grid.RowSpan="11"
                    Grid.Column="18"
                    Grid.ColumnSpan="9"
                    Margin="5,0,0,0"
                    BorderThickness="1">
                    <h:HIconicDisplayObjectWPF IconicObject="{Binding FImage}" />
                </h:HSmartWindowControlWPF>
                <!--<Image
                    Grid.Row="0"
                    Grid.Column="0"
                    Grid.ColumnSpan="4"
                    Width="300"
                    Height="60"
                    Margin="10,10,45,10"
                    HorizontalAlignment="Left" />-->
                <Grid
                    Grid.Row="0"
                    Grid.Column="0"
                    Grid.ColumnSpan="4"
                    Width="300"
                    Height="60"
                    Margin="10,10,10,10"
                    HorizontalAlignment="Left">
                    <Image Source="/CENJE.Data_interface/CENj.png" />
                </Grid>
                <Label
                    Name="labtitle"
                    Grid.Row="0"
                    Grid.Column="9"
                    Grid.ColumnSpan="9"
                    Width="345"
                    Height="64"
                    Margin="6,0,10,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    HorizontalContentAlignment="Center"
                    Background="#FF8A2BE2"
                    Content="钉卷机视觉检测系统"
                    FontSize="36"
                    FontStyle="Normal"
                    Foreground="#FFFFFFFF"
                    RenderTransformOrigin="0.494,0.463" />
                <StackPanel
                    Grid.Row="0"
                    Grid.Column="18"
                    Grid.ColumnSpan="11">
                    <StackPanel Orientation="Horizontal">
                        <Label
                            Name="showCurrentProject"
                            Grid.ColumnSpan="8"
                            Width="100"
                            Height="34"
                            Margin="0,1,35,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            HorizontalContentAlignment="Left"
                            Background="#FFFFFFFF"
                            Content="当前配方："
                            FontSize="20"
                            FontStyle="Normal"
                            Foreground="#FF008000"
                            RenderTransformOrigin="0.494,0.463" />
                        <TextBox
                            Width="150"
                            Height="34"
                            Margin="5,10,0,5"
                            VerticalAlignment="Center"
                            VerticalContentAlignment="Center"
                            FontSize="20"
                            Text="" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Label
                            Margin="1"
                            Content="负极间隔"
                            FontSize="20"
                            Foreground="#FF008000" />
                        <TextBox
                            Name="nIntervalValue"
                            Width="60"
                            Margin="1"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="20" />
                        <Label
                            Margin="1"
                            Content="正极间隔"
                            FontSize="20"
                            Foreground="#FF008000" />
                        <TextBox
                            Name="pIntervalValue"
                            Width="60"
                            Margin="1"
